{
    "include": [
      "./src/**/*"
    ],
    "exclude": [
      "dist",
      ".quasar",
      "node_modules"
    ],
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "boot/*": [
          "src/boot/*"
        ],
        "components/*": [
          "src/components/*"
        ],
        "examples/*": [
          "src/examples/*"
        ],
        "layouts/*": [
          "src/layouts/*"
        ],
        "pages/*": [
          "src/pages/*"
        ]
      }
    }
  }
  